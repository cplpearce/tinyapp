<header>
  <!-- If the user is logged in, change the header -->
  <% try { %>
  <%   email = user.email %>
  <nav class="navbar navbar-expand-md navbar-dark bg-info">
    <img src="/img/meat.png" width="30" height="30" alt="">
    <a class="navbar-brand mx-1" href="/urls">Lil'Links</a>

    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <br>
    <form class="form-inline" method="POST" action="/logout" >
      <div class="input-group">
        <span class=" p-2"><small>Logged in as:</small> <%= email %></span>
      </div>
      <button class="input-group btn btn-warning" type="submit">Log Off</button>
    </form>
    <% } catch (err) { %>
    <nav class="navbar navbar-expand-md navbar-dark bg-info">
      <img src="/img/meat.png" width="30" height="30" alt="">
      <a class="navbar-brand mx-1" href="/urls">Lil'Links</a>
      <br>
      <div class="input-group">
        <span class=" p-2">Not Logged in!</span>
        <button href="/login">Login</button>
        <button href="/register">Register</button>
      </div>
      <a href="/login"><button class="input-group btn btn-warning">Log In</button></a>
    <% } %>
  </nav>
</header>